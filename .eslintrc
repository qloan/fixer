{
    "rules": {
        "indent": [
            2,
            4
            ],
        "key-spacing":      [ 0 ],
        "no-multi-spaces":  [ 0 ],
        "quotes":           [ 0 ],
        "strict":           [ 0 ],
        "no-use-before-define":   [ 0 ],
        "no-mixed-requires": [0],
        "no-underscore-dangle": [0],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "node": true
    }
}
